<?xml version="1.0" encoding="utf-8"?>
<suburls xmlns="http://jelix.org/ns/suburls/1.0">
    <url pathinfo="/404.html" action="error:notfound" />
    <url pathinfo="/403.html" action="error:badright" />
    <url pathinfo="/forms/getdata" action="jforms:getListData" />
    <url pathinfo="/forms/js/:__form/:__fid.js" action="jforms:js">
        <param name="__form" type="string" />
        <param name="__fid" type="string" />
    </url>

    <url pathinfo="/res/:targetmodule/:file" action="www:getfile">
        <param name="file" type="path" />
    </url>

</suburls>